  <!-- 操作详情条件仓库name，操作类别，操作时间，操作人，货物名称 -->
  <select id="selOperDetails" resultType="cn.erp.dto.OperDetailsDto">
  SELECT
	tse. NAME AS ckname,
	tod.type AS cztype,
	tod.operTime AS cztime,
	tep. NAME AS czname,
	tgs. NAME AS goodsname
FROM
	tbl_store tse
INNER JOIN tbl_operdetail tod ON tse.uuid = tod.storeUuid
INNER JOIN tbl_emp tep ON tep.uuid = tse.empuuid
INNER JOIN tbl_goods tgs ON tgs.uuid = tod.goodsUuid
WHERE
	1 = 1
	<if test="ckname != null">
	AND tse. NAME = #{ckname}
	</if>
	<if test="cztype != null">
	AND tod.type = #{cztype}
	</if>
	<if test="starttime != null and endtime != null">
	AND tod.operTime BETWEEN #{starttime} AND #{endtime}
	</if>
	<if test="empuuid != null">
	AND tod.empUuid = #{empuuid}
	</if>
<if test="goodsname != null">
AND tgs. NAME = #{goodsname}
</if>

  </select>
  
